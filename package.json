{
  "name": "tm-wear-admin",
  "private": true,
  "version": "1.0.0",
  "description": "TM Wear Admin App",
  "type": "module",
  "engines": {
    "node": ">=14.17"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "tsc && npm run lint && npm run prettier-check",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint src --fix '**/*.{ts,tsx,md}'",
    "prettier-check": "prettier --check '**/*.{ts,tsx,md}'",
    "prettier-write": "prettier --write '**/*.{ts,tsx,md}'",
    "prepare": "husky install",
    "clean": "rm -rf node_modules build dist package-lock.json yarn.lock pnpm-lock.yaml"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fitzzz/utils": "^0.2.0",
    "@mui/material": "^5.11.10",
    "@mui/x-date-pickers": "^5.0.20",
    "@popperjs/core": "^2.11.6",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-query-persist-client": "^4.24.10",
    "@tanstack/react-table": "^8.7.9",
    "@tanstack/react-virtual": "3.0.0-beta.53",
    "axios": "^1.3.4",
    "class-variance-authority": "^0.4.0",
    "idb-keyval": "^6.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-number-format": "^5.1.4",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.8.1",
    "react-toastify": "^9.1.1",
    "simplebar-react": "^3.2.1",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,md}": [
      "npm run check"
    ]
  }
}